public class Node<E> {
	protected E element;
	protected Node<E> next;
	protected Node<E> previous;
	
	public Node(E e) {
		element = e;
		next = null;
	}

	public E getElement() {
		return element; 
	}

	public Node<E> getNext() { 
		return next;
	}

	public void setElement(E e) { 
		element = e; 
	}

	public void setNext(Node<E> n) {
		next = n; 
	}
	
	@Override
	public String toString() {
		return " Elemento ["+element.toString() + "] proximo [" + (next != null ? next.getElement() : "null" )+ "]";
	}
}

